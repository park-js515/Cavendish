    "eject": "react-scripts eject",
    "sass": "sass --no-source-map --watch src/styles/scss:src/styles/css",
    "sass-default": "sass --no-source-map src/styles/scss:src/styles/css"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
